def importStockList():
    df = ts.get_stock_basics()
    df["code"] = df.index
    df = df.reset_index(drop=True)
    
    db=cx_Oracle.connect('c##stock','didierg160','myoracle')  #创建连接  
    cr=db.cursor()
    
    sql = "delete tb_stock_list"
    cr.execute(sql) 
    
    for row in df.itertuples(): 
        sql = "insert into tb_stock_list values (" 
        getattr(row, "code")   
    
    
    db.commit()        
    cr.close ()  
    db.close ()  
    
    

