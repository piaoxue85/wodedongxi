import tushare as ts
import time 
df = ts.get_h_data('000002',
                   '1990-12-19' ,
                   time.strftime('%Y-%m-%d',time.localtime(time.time())),
                   'D',
                   3,
                   1 
                  )
print(df)
print(df.count())
print(len(df))

