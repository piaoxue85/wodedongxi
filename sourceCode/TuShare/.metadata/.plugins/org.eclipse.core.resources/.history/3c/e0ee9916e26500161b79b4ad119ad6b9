from sqlalchemy import create_engine
import tushare as ts
import time 

df = ts.get_h_data(
                    code        =  '000002',    
                    start       =  '2015-12-19',
                    end         =  '2016-08-19',
                    autype      =  'None',      
                    index       =  False,       
                    retry_count =  3 ,           
                    pause       =  1            
                  )

print(df)
print(df.count())
engine = create_engine('oracle://c##stock:didierg160@myoracle')
df.to_sql('tick_data',engine,if_exists='append')
